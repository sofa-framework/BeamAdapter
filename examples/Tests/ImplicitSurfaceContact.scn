		<noImplicitSurfaceAdaptiveConstraint  friction="0.2" visualization="1" object1="@../Germaine/implicitSurfaceMO" object2="@DOFs" interpolation="@InterpolCoils"	alarmDistance="2.0" radius="@InterpolCoils.radius"/>		



